package net.sqs2.omr.session.model;

import net.sqs2.omr.model.PageTaskException;

public class PageTaskExceptionEntry {
	int index;
	PageTaskException taskException;

	public PageTaskExceptionEntry(int index, PageTaskException taskException) {
		this.index = index;
		this.taskException = taskException;
	}

	public int getIndex() {
		return this.index;
	}

	public PageTaskException getTaskError() {
		return this.taskException;
	}
}
