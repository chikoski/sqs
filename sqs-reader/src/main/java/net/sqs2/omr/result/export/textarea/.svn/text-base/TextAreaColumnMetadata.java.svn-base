package net.sqs2.omr.result.export.textarea;

import net.sqs2.omr.master.FormArea;
import net.sqs2.omr.result.export.ColumnMetadata;

public class TextAreaColumnMetadata extends ColumnMetadata {

	TextAreaColumnMetadata(FormArea primaryFormArea, String sourceDirectoryPath,
			String textAreaDirectoryPath, int columnIndex, int numRows) {
		super(primaryFormArea, sourceDirectoryPath, textAreaDirectoryPath, columnIndex);
	}

	public String getLabel() {
		return this.primaryFormArea.getLabel();
	}

	public String[] getHints() {
		return this.primaryFormArea.getHints();
	}	
}